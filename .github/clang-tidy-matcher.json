{
    "problemMatcher": [
        {
            "owner": "clang-tidy",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*?)\\s+\\[(.*?)\\]$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                },
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+):\\s+note:\\s+(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4,
                    "loop": true
                }
            ]
        }
    ]
}
